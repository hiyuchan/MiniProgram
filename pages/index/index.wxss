@import "../common/mainTabCell/mainTabCell.wxss";
@import "../components/loading/loading.wxss";

page{
  width: 100%;
  background: #ddd;
}
.main_tab{
  width: 100%;
  height: 85rpx;
  border-bottom: 1rpx solid #ddd;
  background: #fff;
  display: flex;
  position: fixed;
  z-index: 99;
}
.tab_item{
  text-align: center;
  line-height: 85rpx;
  flex: 1;
}
.tab_item.active{
  color: #0099ff;
  font-weight: bold;
}
swiper-item image{
  position: relative;
  top: 85rpx;
  width: 100%;
  height: 100%;
}
.scroll{
  width: 100%;
  height: 100%;
}